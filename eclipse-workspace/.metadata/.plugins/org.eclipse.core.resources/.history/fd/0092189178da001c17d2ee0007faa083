package JavaBasics;

public class Practice {
public static void main(String[] args) {
	
	
	//Array writing 
	
	int b[]=new int[7];
	
	//int a[]= {12,5,10,4,17,3,9};
	b[0]=10;
	b[1]=20;
	b[3]=30;
	b[4]=40;
	b[5]=50;
	b[6]=60;
	b[7]=70;
	
	System.out.println(" array length:=" +b.length);
	
	for(int c=0;c< b.length;c++) {
		System.out.println(b[c]);
		
	}
	
	
	
	
	
	
	
}
}
